					"sftp_biometric_linked_properties_typeProperties_userName": {
						"value": "[parameters('sftp_biometric_linked_properties_typeProperties_userName')]"
					},
					"sftpsource_properties_typeProperties_host": {
						"value": "[parameters('sftpsource_properties_typeProperties_host')]"
