		"bioMetricHealthSFTP_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "20.81.27.30"
		},
		"bioMetricHealthSFTP_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": " AZBiometricHealth"
		},
		"blob_biometric_linked_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sabiometric.dfs.core.windows.net"
		},
		"sftp_biometric_linked_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "sjgm3yzmtomr6.eastus.azurecontainer.io"
