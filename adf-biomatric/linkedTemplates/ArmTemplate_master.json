					"batchEditProcessor_properties_typeProperties_batchUri": {
						"value": "[parameters('batchEditProcessor_properties_typeProperties_batchUri')]"
					},
					"batchEditProcessor_properties_typeProperties_poolName": {
						"value": "[parameters('batchEditProcessor_properties_typeProperties_poolName')]"
					}
				}
			},
			"dependsOn": []
